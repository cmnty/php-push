{
    "name": "cmnty/push",
    "description": "Web Push library for PHP",
    "keywords": ["Notifications", "Push Notifications", "Push Messages", "Push", "Web Push", "Web Push API", "Push API"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan de Ruijter",
            "email": "johan@cmnty.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "guzzlehttp/guzzle": "~6.2.1",
        "mdanter/ecc": "~0.4.0",
        "spomky-labs/base64url": "~1.0"
    },
    "require-dev": {
        "ext-crypto": "*",
        "lib-openssl": "*",
        "spomky-labs/php-aes-gcm": "~1.0",
        "phpspec/phpspec": "~3.2",
        "phpspec/prophecy": "~1.6"
    },
    "suggest": {
        "ext-crypto": "Use ext-crypto to encrypt the notifications",
        "lib-openssl": "Use lib-openssl to encrypt the notifications",
        "spomky-labs/php-aes-gcm": "Use native php to encrypt the notifications. This is significantly slower than the alternatives."
    },
    "autoload": {
        "psr-4": {
            "Cmnty\\Push\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
